{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./vite-entry.cjs",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "vitest",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\" README.md schema.graphql",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "run-s \"lint --fix --cache\"",
    "download-schema": "run-s 'download-schema:*'",
    "download-schema:script": "node ./bin/download-schema.mjs",
    "download-schema:format": "prettier --write schema.graphql",
    "graphql": "run-s download-schema 'graphql:*'",
    "graphql:codegen": "graphql-codegen",
    "graphql-watch": "graphql-codegen --watch",
    "preview": "vite preview"
  },
  "dependencies": {
    "@apollo/client": "^3.8.2",
    "@chakra-ui/react": "^2.8.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@hookform/resolvers": "^3.3.2",
    "@react-google-maps/api": "^2.19.2",
    "apollo-upload-client": "^17.0.0",
    "chakra-react-select": "^4.7.5",
    "cross-fetch": "^4.0.0",
    "effect": "^2.0.0-next.50",
    "framer-motion": "^10.16.4",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "react": "^18.2.0",
    "react-datepicker": "^4.21.0",
    "react-dom": "^18.2.0",
    "react-google-autocomplete": "^2.7.3",
    "react-hook-form": "^7.47.0",
    "react-icons": "^4.11.0",
    "react-router-dom": "6.16.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-inspector/graphql-cli-common": "^2.9.0",
    "@parcel/watcher": "^2.3.0",
    "@types/apollo-upload-client": "^17.0.4",
    "@types/node": "^20.5.9",
    "@types/react": "^18.2.21",
    "@types/react-datepicker": "^4.19.1",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "@vitejs/plugin-react": "^4.0.3",
    "dotenv-flow": "^3.3.0",
    "eslint": "^8.48.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jsdom": "^22.1.0",
    "node-fetch": "^3.3.2",
    "npm-run-all": "^4.1.5",
    "prettier": "3.0.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.5",
    "vitest": "^0.34.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "yarn@3.6.4"
}
